# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Your Supabase project URL
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Service role key for server-side operations (keep secret!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Optional: Database URL for direct connections
DATABASE_URL=postgresql://postgres:[password]@[host]:[port]/[database]

# RAVYZ App Configuration
NEXT_PUBLIC_APP_NAME=RAVYZ
NEXT_PUBLIC_APP_URL=https://your-app-domain.com

# Analytics (optional)
NEXT_PUBLIC_GA_TRACKING_ID=GA-XXXXXXXXX

# n8n Webhook Integration
# Your n8n instance webhook URL
NEXT_PUBLIC_N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook

# Optional: n8n API key for authenticated requests
N8N_API_KEY=your-n8n-api-key-here

# n8n Configuration
N8N_INSTANCE_URL=https://your-n8n-instance.com
N8N_EXECUTION_TIMEOUT=30000